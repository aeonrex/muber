{
  "VERSION": "v1",
  "SERVER_PORT": 8080,
  "RESOURCES": {
    "STOP": {
      "CONTROLLER": "stops",
      "ROUTES": {
        "/stops": [
          {
            "METHOD": "GET",
            "VALUE": "getMany"
          }
        ],
        "/stops/:id": [
          {
            "METHOD": "GET",
            "VALUE": "getOne"
          }
        ]
      }
    },
    "DEPARTURE": {
      "CONTROLLER": "departures",
      "ROUTES": {
        "/stops/:id/departures": [
          {
            "METHOD": "GET",
            "VALUE": "getForStop"
          }
        ]
      }
    }
  }
}
